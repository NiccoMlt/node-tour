{
    "name": "node-tour",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "build": "npm run type-check && npm run build:js",
        "build:ts": "tsc",
        "build:js": "babel backend/src --out-dir ./backend/dist --extensions \".js,.ts,.tsx\" --source-maps inline",
        "build:types": "tsc --declaration --emitDeclarationOnly",
        "lint": "npm run lint:js && npm run lint:ts",
        "lint:js": "eslint backend",
        "lint:ts": "tslint --project .",
        "pretty": "npx prettier --write \"./**/*.{ts,tsx,js,jsx,json}\"",
        "start": "npm run build && node ./bin/www",
        "tslint-check": "tslint-config-prettier-check ./tslint.json",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch"
    },
    "dependencies": {
        "cookie-parser": "~1.4.3",
        "debug": "~2.6.9",
        "express": "~4.16.0",
        "morgan": "~1.9.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.3.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-typescript": "^7.1.0",
        "@types/cookie-parser": "^1.4.1",
        "@types/express": "^4.16.1",
        "@types/morgan": "^1.7.35",
        "@types/node": "^10.12.21",
        "eslint": "^5.13.0",
        "eslint-config-prettier": "^4.0.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "husky": "^1.3.1",
        "prettier": "1.16.4",
        "pretty-quick": "^1.10.0",
        "tslint": "^5.12.1",
        "tslint-config-prettier": "^1.17.0",
        "tslint-config-standard-plus": "^2.1.3",
        "tslint-eslint-rules": "^5.4.0",
        "typescript": "^3.3.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/NiccoMlt/node-tour.git"
    },
    "homepage": "https://github.com/NiccoMlt/node-tour#readme"
}
